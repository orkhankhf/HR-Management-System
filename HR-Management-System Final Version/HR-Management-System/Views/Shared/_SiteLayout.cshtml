		function show_notice(id) {
			$('.show-notice-title').html('<div class="text-center"><img src="http://froiden.cloudapp.net/hrm_demo/front_assets/img/loading-spinner-blue.gif"></div>');
			$('#show-notice-body').html('<div class="text-center"><img src="http://froiden.cloudapp.net/hrm_demo/front_assets/img/loading-spinner-blue.gif"></div>');

			$.ajax({
				type: 'POST',
				url: "http://froiden.cloudapp.net/hrm_demo/dashboard/notice/" + id + "",
				cache: false,
				data: { "id": id },
				success: function (response) {

					$('.show-notice-title').html(response.title);
					$('#show-notice-body').html(response.description);

				},
				error: function (xhr, textStatus, thrownError) {
					alert('Something went wrong. Please Try again later!');
				}
			});

		}

