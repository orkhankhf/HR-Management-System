	<script>

// Show change password modal body

		 function change_password()
		 {
			 if ($("#password_confirmation").val() == "" || $("#password").val() == "") {
				 $('#error').html('<div class="alert alert-danger alert-dismissable">Password cannot be empty!</div>');
			 }else if ($("#password_confirmation").val() == $("#password").val()) {
				$('#error').html('<img src="http://froiden.cloudapp.net/hrm_demo/front_assets/img/loading-spinner-blue.gif">');
				$('#error').attr('class','text-center');
				$.ajax({
					type:'POST',
					url: "@Url.Action("ChangePassword", "Employees")",
					dataType: 'json',
					data: { new_password: $("#password_confirmation").val()}
				}).done(function(response)
				{
					if(response.status=='success')
					{
						$('.field').hide();
						$('#error').html('<div class="alert alert-success"><span class="fa fa-check"></span> '+response.msg+'</div>');
						setTimeout(function () {
							window.location.href = '@Url.Action("Login")';
						}, 2000);

					}else if(response.status == "error")
					{

						$('#error').html('<div class="alert alert-danger alert-dismissable">' + response.msg + '</div>');

					}

				});
			 } else {
				 $('#error').html('<div class="alert alert-danger alert-dismissable">Passwords should be same !</div>');
			 }
		 }
		 function create_leave_app() {
			 $.ajax({
				type:'POST',
				url: "@Url.Action("CreateLeaveApp", "Employees")",
				dataType: 'json',
				data: { leave_date: $(".leave_date").val(), leave_reason: $(".leave_reason").val()}
			 }).done(function(response)
			 {
				if(response.status=='success')
				{
					$('#leave_error').html('<div class="alert alert-success"><span class="fa fa-check"></span> '+response.msg+'</div>');
					setTimeout(function () {
						$('.apply_modal').modal('hide');
					}, 2000);

				}else if(response.status == "error")
				{
					$('#leave_error').html('<div class="alert alert-danger alert-dismissable">' + response.msg + '</div>');

				}

			 });
		 }
	</script>

